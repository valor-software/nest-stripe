  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Nest Stripe Test",
      "type": "node",
      "port": 9229,
      "request": "attach",
      "cwd": "${workspaceFolder}/apps/nest-stripe-test/src/"
    },
    {
      "name": "Attach By Process ID",
      "type": "node",
      "request": "attach",
      "processId": "${command:PickProcess}",
      "skipFiles": ["<node_internals>/**"]
    },
  ]
}